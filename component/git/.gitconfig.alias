[alias]
  a = add
  aa = add .
  ######################
  b = branch -v
  ba = branch -a -vv
  bd = branch -D
  ######################
  c = commit -m
  ca = commit --amend
  ######################
  cl = clone
  ######################
  cp = cherry-pick
  cpa = cherry-pick --abort
  cpc = cherry-pick --continue
  cps = cherry-pick --skip
  ######################
  cf = config
  cfl = config --list
  ######################
  ch = checkout
  chb = checkout -b
  ######################
  d = diff -u
  ######################
  f = fetch
  fp = fetch -p
  ######################
  p = push
  po = push origin
  ######################
  r = reset
  rh = reset HEAD
  rh1 = reset HEAD^
  rh2 = reset HEAD^^
  rh3 = reset HEAD^^^
  rhard = reset --hard
  rhhard = reset --hard HEAD
  ######################
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbs = rebase --skip
  rbi = rebase -i
  ######################
  rema = remote add
  remao = remote add origin
  remd = remote remove
  remdo = remote remove origin
  ######################
  ref = reflog
  ######################
  s = status
  ######################
  st = stash
  sta = stash -u
  stp = stash push
  stl = stash list
  stpop = stash pop
  stshow = stash show
  stdrop = stash drop
  ######################
  t = tag
  td = tag -d
